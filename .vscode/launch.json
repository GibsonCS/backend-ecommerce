{
    "configurations": [
    {
        "name": "Launch via NPM",
        "request": "launch",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
            "run-script",
            "test:debug"
        ],
        "type": "node",
        "skipFiles": [
            "<node_internals>/**",
            "${workspaceFolder}/node_modules/**/*.js"
        ],
      "console": "integratedTerminal"  
    },
    ]
} 
